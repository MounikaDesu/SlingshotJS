{
  "name": "slingshot-sagas",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:debug": "mocha tools/testSetup.js \"src/**/*.spec.js\" --debug-brk",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"./src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "Stephen Collins",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "~6.16.0",
    "bootstrap": "~3.3.7",
    "font-awesome": "4.6.3",
    "history": "3.2.1",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jquery": "~3.1.1",
    "jwt-decode": "~2.1.0",
    "react": "~15.3.2",
    "react-bootstrap": "~0.30.5",
    "react-dom": "~15.3.2",
    "react-redux": "~4.4.5",
    "react-router": "3.0.0-beta.1",
    "react-router-redux": "~4.0.6",
    "redux": "~3.6.0",
    "redux-form": "6.2.0",
    "redux-saga": "~0.12.0",
    "reselect": "~2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "~6.5.0",
    "babel-cli": "~6.16.0",
    "babel-core": "~6.17.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-react-display-name": "~2.0.0",
    "babel-plugin-transform-object-rest-spread": "~6.16.0",
    "babel-plugin-transform-react-constant-elements": "~6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "~0.2.9",
    "babel-preset-latest": "~6.16.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-2": "~6.17.0",
    "babel-register": "~6.16.3",
    "browser-sync": "~2.17.0",
    "chai": "~3.5.0",
    "chalk": "~1.1.3",
    "connect-history-api-fallback": "~1.3.0",
    "coveralls": "~2.11.14",
    "cross-env": "~3.0.0",
    "css-loader": "~0.25.0",
    "enzyme": "~2.4.1",
    "eslint": "~3.7.0",
    "eslint-plugin-import": "~2.0.0",
    "eslint-plugin-jsx-a11y": "~2.2.2",
    "eslint-plugin-react": "~6.3.0",
    "eslint-watch": "~2.1.14",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
